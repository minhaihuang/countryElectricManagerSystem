<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hhm.elec.domain.ElecText" table="elecText">
		<id name="textId" >
			<generator class="uuid"></generator>
		</id>	
			
		<property name="textName" column="textName"></property>
		<property name="textComment" column="textComment"></property>
		<property name="textDate" column="textDate"></property>
	</class>
</hibernate-mapping>