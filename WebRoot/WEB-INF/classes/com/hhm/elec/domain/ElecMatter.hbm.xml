<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hhm.elec.domain.ElecMatter" table="ElecMatter">
		<id name="matterId" >
			<generator class="uuid"></generator>
		</id>	
			
		<property name="stationRunStatus" column="stationRunStatus"></property>
		<property name="devRunStatus" column="devRunStatus"></property>
		<property name="createDate" column="createDate"></property>
	</class>
</hibernate-mapping>