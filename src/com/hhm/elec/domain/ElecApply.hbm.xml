<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.hhm.elec.domain.ElecApply" table="ElecApply">
		<id name="applyId">
			<generator class="uuid"></generator>
		</id>
		<property name="userId" column="userId"></property>
		<property name="username" column="username"></property>
		<property name="account" column="account"></property>
		<property name="processDefinitionId" column="processDefinitionId"></property>
		<property name="processDefinitionKey" column="processDefinitionKey"></property>
		<property name="applyTime" column="applyTime"></property>
		<property name="applyStatus" column="applyStatus"></property>
		<property name="fileName" column="fileName"></property>
		<property name="path" column="path"></property>
	</class>
</hibernate-mapping>